    #include<stdio.h>
    void main()
    {
int a[5],i,j,temp;
printf("enter rhe element \n");
for ( i = 0; i <=4; i++)
scanf("%d",&a[i]);

for ( i = 0; i <=4; i++)
{
    for ( j = 0; j <4-i; j++)
    {
if(a[j]>a[j+1])    
    temp=a[j];
   a [j+1]=a[j];
   a[j]=temp;

}
} 
for ( i = 0; i <=4; i++)

printf("%d",a[i]);

    }